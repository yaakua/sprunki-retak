[{"question": "Ball Clicker \uac8c\uc784\uc744 \uc5b4\ub5bb\uac8c \uc2dc\uc791\ud569\ub2c8\uae4c?", "answer": "Ball Clicker\ub294 \ud654\uba74 \uc911\uc2ec\uc5d0 \uc791\uc740 \ud2c8\uc774 \uc788\ub294 \uc6d0\uc73c\ub85c \uc2dc\uc791\ud569\ub2c8\ub2e4. \ubaa9\ud45c\ub294 \uc774 \uc6d0 \uc548\uc5d0\uc11c \ud074\ub9ad\ud558\uc5ec \uacf5\uc744 \uc0dd\uc131\ud558\ub294 \uac83\uc785\ub2c8\ub2e4. \ucc98\uc74c\uc5d0\ub294 \uc81c\ud55c\ub41c \uacf5 \uc218\ub7c9\uc73c\ub85c \uc2dc\uc791\ud558\uba70 \uc2dc\uac04\uc774 \ud750\ub974\uba74\uc11c \uc99d\uac00\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4."}, {"question": "Ball Clicker\uc758 \ubcf4\uc0c1\uc740 \ubb34\uc5c7\uc785\ub2c8\uae4c?", "answer": "\uacf5\uc774 \uc6d0\uc5d0\uc11c \ube60\uc838\ub098\uc640 \ub5a8\uc5b4\uc9c8 \ub54c \uac12\ube44\uc2fc \ubcf4\uc0c1\uc744 \uac00\uc838\uc635\ub2c8\ub2e4. \uc774\ub7ec\ud55c \ubcf4\uc0c1\uc740 \ub354 \ub9ce\uc740 \uacf5\uc744 \ucd94\uac00\ud558\uac70\ub098 \uc218\uc785\uc744 \uc99d\ub300\uc2dc\ud0a4\uac70\ub098 \ud2c8\uc744 \ub113\ud788\uac70\ub098 \ucd94\uac00\uc801\uc778 \uc6d0\uc744 \uc0dd\uc131\ud558\ub294 \ub4f1 \uac15\ub825\ud55c \uc5c5\uadf8\ub808\uc774\ub4dc\ub97c \uc7a0\uae08 \ud574\uc81c\ud558\ub294 \ub370 \uc0ac\uc6a9\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4."}, {"question": "Ball Clicker\uc5d0\uc11c \uacf5\uc758 \uc218\ub7c9\uc744 \uc5b4\ub5bb\uac8c \uc99d\uac00\uc2dc\ud0ac \uc218 \uc788\uc2b5\ub2c8\uae4c?", "answer": "\uac8c\uc784\uc744 \ud558\uba74\uc11c \uc2dc\uac04\uc774 \ud750\ub974\uba74\uc11c \uacf5\uc758 \uc218\ub7c9\uc744 \uc99d\uac00\uc2dc\ud0ac \uc218 \uc788\uc2b5\ub2c8\ub2e4. \ub610\ud55c, x3 \ubc30\uc728\uc744 \ud074\ub9ad\ud558\uc5ec \uc81c\ud55c\ub41c \uc2dc\uac04 \ub3d9\uc548 \uacf5\uc758 \uc218\ub7c9\uacfc \uc218\uc775\uc744 \uc99d\ud3ed\uc2dc\ud0a4\ub294 \uae30\ud68c\ub97c \ucc3e\uc544\uc57c \ud569\ub2c8\ub2e4."}, {"question": "Ball Clicker\uc5d0\uc11c \ud68c\uc804\ud558\ub294 \uc6d0\uc758 \ubaa9\uc801\uc740 \ubb34\uc5c7\uc785\ub2c8\uae4c?", "answer": "Ball Clicker\uc5d0\uc11c \ud68c\uc804\ud558\ub294 \uc6d0\uc740 \uacf5\uc744 \uc0dd\uc131\ud558\uace0 \ud280\uac8c \uc720\uc9c0\ud558\ub294 \uacf3\uc785\ub2c8\ub2e4. \uc6d0 \uc548\uc758 \uacf5\uc774 \ud280\ub0a0 \ub54c\ub9c8\ub2e4 \uadf8 \uac00\uce58\uac00 \uc99d\uac00\ud569\ub2c8\ub2e4. \uadf8\ub7ec\ub098 \uc608\uce21\ud560 \uc218 \uc5c6\ub294 \ud68c\uc804\uc73c\ub85c \uc778\ud574 \uacf5\uc758 \uc6c0\uc9c1\uc784\uc744 \uc81c\uc5b4\ud558\uae30\uac00 \uae4c\ub2e4\ub86d\uc2b5\ub2c8\ub2e4."}, {"question": "Ball Clicker\uc5d0\uc11c \uc5c5\uadf8\ub808\uc774\ub4dc\ub294 \uc5b4\ub5bb\uac8c \uc791\ub3d9\ud569\ub2c8\uae4c?", "answer": "Ball Clicker\uc758 \uac01 \uc5c5\uadf8\ub808\uc774\ub4dc(\ub354 \ub9ce\uc740 \uacf5\uc744 \ucd94\uac00\ud558\uac70\ub098 \uc218\uc785\uc744 \uc99d\ub300\uc2dc\ud0a4\ub294 \ub4f1)\ub294 \ucda9\ubd84\ud55c \uc810\uc218\ub97c \ud68d\ub4dd\ud560 \ub54c\uae4c\uc9c0 \uc7a0\uaca8 \uc788\uc2b5\ub2c8\ub2e4. \uc774\ub7ec\ud55c \ub354 \uac15\ub825\ud55c \uae30\ub2a5\uc744 \uc7a0\uae08 \ud574\uc81c\ud558\ub294 \ub370 \uc804\ub7b5\uc774 \uc911\uc694\ud569\ub2c8\ub2e4."}, {"question": "Ball Clicker\uc758 \ucee8\ud2b8\ub864\uc740 \ubb34\uc5c7\uc785\ub2c8\uae4c?", "answer": "Ball Clicker\uc758 \ucee8\ud2b8\ub864\uc740 \uac04\ub2e8\ud569\ub2c8\ub2e4. \uac8c\uc784\uc744 \ud558\ub824\uba74 \uc67c\ucabd \ub9c8\uc6b0\uc2a4 \ubc84\ud2bc\uc744 \uc0ac\uc6a9\ud569\ub2c8\ub2e4. \uc6d0 \uc548\uc5d0\uc11c \uacf5\uc744 \uc0dd\uc131\ud558\uae30 \uc704\ud574 \ud074\ub9ad\ud569\ub2c8\ub2e4."}, {"question": "Ball Clicker\uc5d0\uc11c \ub09c\uc774\ub3c4\ub294 \uc5b4\ub5bb\uac8c \uc99d\uac00\ud569\ub2c8\uae4c?", "answer": "Ball Clicker\uc5d0\uc11c \uac8c\uc784\uc774 \uc9c4\ud589\ub420 \ub54c \ub09c\uc774\ub3c4\uac00 \ub192\uc544\uc9d1\ub2c8\ub2e4. \uc5ec\ub7ec \uac1c\uc758 \ud68c\uc804\ud558\ub294 \uc6d0\uacfc \uac01 \ub808\ubca8\ub9c8\ub2e4 \ub354 \ube60\ub974\uac8c \uc6c0\uc9c1\uc774\ub294 \uacf5\uc744 \ub9cc\ub098\uac8c \ub418\uba70, \ud0c0\uc774\ubc0d, \uc704\uce58, \ud280\ub294 \ud328\ud134\uc744 \ub9c8\uc2a4\ud130\ud558\ub294 \uac83\uc774 \ub2f9\uc2e0\uc5d0\uac8c \ub3c4\uc804\uc774 \ub429\ub2c8\ub2e4."}, {"question": "Ball Clicker\uc5d0\uc11c \uacf5\uc744 \uc870\uc900\ud558\ub294 \ud301\uc774 \uc788\uc2b5\ub2c8\uae4c?", "answer": "\uc608, \uc6d0\uc758 \uac00\uc7a5 \uac12\ube44\uc2fc \ubd80\ubd84\uc744 \ub9de\ucd94\uae30 \uc704\ud574 \uacf5\uc744 \uc815\ud655\ud558\uac8c \uc870\uc900\ud558\uc138\uc694. \uc774\ub807\uac8c \ud558\uba74 \uc5c4\uccad\ub09c \uc810\uc218\ub97c \uc313\uace0 \uc810\uc218\ub97c \ub192\uc77c \uc218 \uc788\uc2b5\ub2c8\ub2e4."}, {"question": "Ball Clicker\ub97c \ubaa8\ubc14\uc77c\uc5d0\uc11c \ud50c\ub808\uc774\ud560 \uc218 \uc788\uc2b5\ub2c8\uae4c?", "answer": "\uc608, Ball Clicker\ub294 \ub370\uc2a4\ud06c\ud1b1\ubfd0\ub9cc \uc544\ub2c8\ub77c \ubaa8\ubc14\uc77c\uc5d0\uc11c\ub3c4 \ud50c\ub808\uc774\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4. \uc774\uac83\uc740 \uc6f9 \ube0c\ub77c\uc6b0\uc800 \uae30\ubc18\uc758 \uac8c\uc784\uc785\ub2c8\ub2e4."}, {"question": "Ball Clicker\uac00 \ub9c8\uc9c0\ub9c9\uc73c\ub85c \uc5c5\ub370\uc774\ud2b8\ub41c \uc2dc\uae30\ub294 \uc5b8\uc81c\uc785\ub2c8\uae4c?", "answer": "Ball Clicker\ub294 2024\ub144 7\uc6d4 26\uc77c\uc5d0 \ub9c8\uc9c0\ub9c9\uc73c\ub85c \uc5c5\ub370\uc774\ud2b8\ub418\uc5c8\uc2b5\ub2c8\ub2e4."}]